{
    "name": "stargazer-team/yii-doctrine-fixture",
    "description": "Yii3 integration Doctrine fixture extension",
    "type": "library",
    "keywords": ["yii", "doctrine", "fixture"],
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/stargazer-team/yii-doctrine-fixture"
    },
    "authors": [
        {
            "name": "Stargazer Team",
            "email": "fcnybok@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "stargazer-team/yii-doctrine": "^1.0.0",
        "doctrine/data-fixtures": "^1.7.0"
    },
    "require-dev": {
        "roave/infection-static-analysis-plugin": "^1.29",
        "vimeo/psalm": "^4.30|^5.9",
        "rector/rector": "^0.15.25"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Yii\\DoctrineFixture\\": "src"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": true,
            "composer/package-versions-deprecated": true,
            "yiisoft/config": true
        }
    },
    "extra": {
        "config-plugin-options": {
            "source-directory": "config"
        },
        "config-plugin": {
            "params": "params.php",
            "di-console": "di-console.php"
        }
    }
}
